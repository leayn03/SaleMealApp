<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="fill_parent"
              android:layout_height="fill_parent"
              android:orientation="vertical"
              android:background="@color/whiteBg" >
    <RelativeLayout
            android:layout_width="fill_parent"
            android:id="@+id/detailLayoutTitle"
            android:layout_height="55dip"
            android:background="@color/frame">
        <ImageButton
                android:id="@+id/detailReturnButton"
                android:src="@drawable/pre_btn"
                android:background="@null"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_centerVertical="true"/>
        <TextView
                android:id="@+id/fileText"
                android:text="@string/orderListDetail"
                android:background="@color/frame"
                android:layout_width="fill_parent"
                android:layout_height="55dip"
                android:layout_centerVertical="true"
                android:gravity="center_vertical"
                android:layout_marginLeft="150dp"
                android:layout_marginTop="20dp"
                android:textColor="@color/titleText"
                android:textSize="20sp"/>
    </RelativeLayout>


    <ListView
        
            android:id="@+id/orderList"
            android:layout_height="wrap_content"
            android:layout_width="fill_parent"
            android:layout_marginTop="55dip"
            android:layout_marginBottom="55dip"

            />

    
    <RelativeLayout 
        android:id="@+id/relativeBottom"
         android:layout_width="fill_parent"
         android:layout_height="55dp"
	      android:orientation="horizontal"
	      android:layout_alignParentBottom="true"
	      android:background="@color/bottomGray"
	      android:padding="5dp">
            <Button
            android:id="@+id/acceptSaleButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="60dip"
            android:layout_alignParentLeft="true"
            android:textSize="18sp"
            android:text="@string/acceptOrderList"
            android:textColor="@color/btnTextColor"
            android:padding="5dip"
            android:background="@drawable/button_click"
            />
           <Button
            android:id="@+id/returnSaleButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="60dip"
            android:layout_alignParentRight="true"
            android:textSize="18sp"
            android:text="@string/cancellReturn"
            android:textColor="@color/btnTextColor"
            android:padding="5dip"
            android:background="@drawable/button_click"
          
            />
        
    </RelativeLayout>

</RelativeLayout>